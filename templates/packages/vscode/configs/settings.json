{
  "[dockerfile]": {"editor.formatOnSave": true},
  "[go]": {"editor.formatOnSave": true},
  "[groovy]": {"editor.formatOnSave": true},
  "[json]": {"editor.formatOnSave": false},
  "[markdown]": {"editor.formatOnSave": true},
  "[plaintext]": {"editor.formatOnSave": true},
  "[shellscript]": {"editor.formatOnSave": true},
  "[terraform]": {"editor.formatOnSave": true},
  "[toml]": {"editor.formatOnSave": true},
  "[yaml]": {"editor.formatOnSave": true},
  "editor.fontFamily": "Hack",
  "editor.rulers": [79, 99],
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "gitlens.codeLens.enabled": false,
  "keyboard.touchbar.enabled": false,
  "python.linting.flake8Enabled": true,
  "shellcheck.executablePath": "/usr/local/bin/shellcheck",
  "shellcheck.exclude": ["SC1091"],
  "terraform.indexing": {
    "enabled": false,
    "liveIndexing": false,
    "delay": 500,
    "exclude": [".terraform/**/*", "**/.terraform/**/*"]
  },
  "terraform.languageServer": {"enabled": true, "args": []},
  "todohighlight.include": ["**/*.js", "**/*.txt", "**/*.md", "**/*.html", "**/*.go", "**/*.sh"],
  "todohighlight.isCaseSensitive": true,
  "todohighlight.isEnable": true,
  "todohighlight.keywordsPattern": "[Oo]bviously|[Bb]asically|[Ss]imply|[Oo]f course|[Cc]learly|[Ee]veryone knows|[Ee]asy|[Ee]asily",
  "window.restoreWindows": "all",
  "window.zoomLevel": 0,
  "workbench.startupEditor": "newUntitledFile",
  "workbench.colorTheme": "Monokai"
}
